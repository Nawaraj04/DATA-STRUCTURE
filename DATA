#include<stdio.h>
int linearSearch(int arr[],int size, int target)
{
	for(int i=0;i<size;i++)
	{
		if(arr[i]==target)
		{
			return i;
		}
	}
	return -1;
}
int main()
{
	int arr[10],s,t;
	printf("Enter 10 numbers\n");
	for(int i=0;i<10;i++)
	{
	scanf("%d",&arr[i]);
	}
	printf("Enter the value of t:\n");
	scanf("%d",&t);
	int a= linearSearch(arr,10,t);
	printf("%d",a);
}
